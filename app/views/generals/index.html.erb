<div id="imgIzq">
        
        <img src="img/logo.jpg" style="margin-left:17px; "/>
        <img src="img/o.png" style="margin-left:20px; margin-top:95%;"/>
       
	
    </div>

	<div id="content" style="left:300px; background-image:url(img/cuadros/amarillo.png); padding:10px; width:1300px;">
          <h1 style=" color:black; ">Lista de Profesionales Registrados</h1>
          
          <table border=1 style="font-size:10pt;">
            <tr style="color:black; ">
              <th>Nombre Empresa</th>
              <th>Nombre Integrante 1</th>
              <th>Nombre Integrante 2</th>
              <th>Nombre Integrante 3</th>
              <th>Logotipo</th>
            </tr>
          
          <% @generals.each do |general| %>
            <tr style=" color:black; ">
              <td><%=h general.nombreEmpresa %></td>
              <td><%=h general.nombreIntegrante1 %></td>
              <td><%=h general.nombreIntegrante2 %></td>
              <td><%=h general.nombreIntegrante3 %></td>
              <td><%=h general.logotipo %></td>
              <td><%= link_to 'Mostrar', general, :style => "color:black" %></td>
              <td><%= link_to 'Editar', edit_general_path(general), :style => "color:black" %></td>
              <td><%= link_to 'Eliminar', general, :confirm => 'Seguro?', :method => :delete, :style => "color:black" %></td>
            </tr>
          <% end %>
          </table>
          
          <br />
		<p><%= link_to "Regresar", administracion_path, :style => "color:black"  %></p>
          
</div>